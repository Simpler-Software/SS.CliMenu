    "PowerShell Core": {
      "commandName": "Executable",
      "executablePath": "pwsh.exe",
      "commandLineArgs": "-NoExit -ExecutionPolicy ByPass -Command \"Import-Module '$(TargetDir)$(TargetName).psd1'\"",
      "workingDirectory": "%UserProfile%"
    },
